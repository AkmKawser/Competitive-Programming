#include <bits/stdc++.h>

using namespace std;

void bogosort(int A[], int N) ///O((n+1)!)
{
    bool stop;

    do
    {
        stop = true;

        for (int i = 1; i < N && stop == true; ++i)
            if (A[i - 1] > A[i])
                stop = false;

    } while (stop == false && next_permutation(A, A + N));
}

int main()
{
    return 0;
}
