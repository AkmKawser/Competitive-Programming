#include <iostream>
#include <fstream>
#include <vector>
#include <cstring>
#include <algorithm>

using namespace std;

class SumOfPower
{
public:

    int findSum(vector <int> a)
    {
        long long sum = 0;
        int N = a.size();

        for ( int i = 0; i < N; ++i )
        {
            long long ans = 0;

            for( int j = i; j < N; ++j )
            {
                ans += a[j];
                sum += ans;
            }
        }

        return sum;
    }
};

int main()
{

}
